{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/"
    },
    { "src": "^/static/(.*)", "dest": "/static/$1" },
    { "src": ".*", "dest": "/index.html" }
  ],
  "env": {
    "MONGODB_URI": "@mongodb-uri",
    "GITHUB_CLIENT_ID": "@github-id",
    "GITHUB_CLIENT_SECRET": "@github-secret"
  }
}
